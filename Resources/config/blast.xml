<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    
    <services>
        <service id="blast.form.type" class="%blast.form.type.class%"></service>
        <service id="blast.request" class="%blast.request.class%">
            <argument type="service" id="service_container" />
        </service>
        
        <service id="twig.extension.split" class="Genouest\Bundle\BlastBundle\Extension\SplitExtension">
            <tag name="twig.extension" />
        </service>
    </services>

</container>
