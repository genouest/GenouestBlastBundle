{
    "name": "genouest/blast-bundle",
    "description": "This bundle allows you to run BLAST+ from a convenient web interface.",
    "keywords": ["genouest", "blast"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "genouest",
            "homepage": "http://www.genouest.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": ">=2.2",
        "genouest/scheduler-bundle": "dev-master",
        "genouest/bioinfo-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Genouest\\Bundle\\BlastBundle": ""
        }
    },
    "target-dir" : "Genouest/Bundle/BlastBundle",
    "repositories": [
        {
            "type": "vcs",
            "url":  "gitolite@chili.genouest.org:sf2-bioinfobundle"
        },
        {
            "type": "vcs",
            "url":  "gitolite@chili.genouest.org:sf2-schedulerbundle"
        }
    ]
}
